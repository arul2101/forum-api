{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "import/no-extraneous-dependencies": "off",
        "eol-last": "off",
        "camelcase": "off",
        "no-unused-vars": "off",
        "class-methods-use-this": "off",
        "no-undef": "off",
        "max-len": "off"
    }
}